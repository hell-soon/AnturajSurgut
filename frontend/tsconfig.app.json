{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "#/*": [
        "./src/*"
      ]
    },
    "resolveJsonModule": true,
    "noEmit": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.vue"
  ]
}
