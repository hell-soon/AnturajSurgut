{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "extends": [
    "@tsconfig/node20/tsconfig.json"
  ],
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": [
      "node"
    ],
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": [
    "vite.config.ts"
  ]
}
